#, c-format
msgid "invalid option '-%c'\n"
msgstr "잘못된 옵션 '-%c'\n"

#: src/pacman/pacman.c:958
#, c-format
msgid "invalid option '--%s'\n"
msgstr "잘못된 옵션 '--%s'\n"

